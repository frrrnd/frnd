{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@utils/*": ["src/utils/*"],
      "@partials/*": ["src/partials/*"],
      "@components/*": ["src/components/*"]
    }
  }
}